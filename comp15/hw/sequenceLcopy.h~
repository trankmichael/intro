typedef char ElementType;

#include <iostream>
using namespace std;

struct Node
{
    ElementType value;
    Node* next;
};


class sequence
{
    
    public:
	
	sequence();
	sequence( ElementType x );
	bool isEmpty();
	int size();
	void concatenate( Node* sequenceAdded );
	ElementType first( );
	Node* rest();
	
	
    private:
	Node* head;
	
	
	
};